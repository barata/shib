{
    "id": "platform-shib",
    "cmd":null,
    "cpus": 0.3,
    "mem": 512.0,
    "type": "DOCKER",
    "instances": 1,
    "volumes": [],
    "container":{
      "docker": {
        "image": "docker-registry.chaordicsystems.com:5000/platform-shib:latest",
        "network": "BRIDGE",
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 0,
            "servicePort": 0,
            "protocol": "tcp"
          }
        ],
        "privileged": false,
        "parameters": [],
        "forcePullImage": true
      }
    },
    "healthChecks":[
      {
        "path": "/status",
        "protocol": "HTTP",
        "portIndex": 0,
        "gracePeriodSeconds": 300,
        "intervalSeconds": 20,
        "timeoutSeconds": 5,
        "maxConsecutiveFailures": 3
      }
    ]
  }
